NODE_ENV=development
APP_NAME=auth
PORT=3000
LOG_DIR=./logs
LOG_FORMAT=dev
ORIGIN=*
CREDENTIALS=false
# generate private key and public key http://travistidwell.com/jsencrypt/demo/ (2048bits)
# encode both to base64 https://www.base64encode.org
PRIVATE_
PUBLIC_
REFRESH_SECRET_
REFRESH_PUBLIC_
PROMISE_CONCURRENCY=10

PG_CONNECTION="postgres://apart:secret@127.0.0.1:5432/auth"
DB_TIMEOUT=200
CONNECTION_KEEP_ALIVE_TIMEOUT=60000
CONNECTION_POOL_SIZE=20
DB_DEBUG=true

KAFKA_HOST_MICROSERVICES="127.0.0.1:9092"
KAFKA_PRODUCE_EVENTS=true
# CONSUME_EVENTS=true
# KAFKA_WORKERS_MAX_BYTES_PER_PARTITION=5243

CACHE_HOST="127.0.0.1"
CACHE_PORT=6379
CACHE_PREFIX="apart-auth"




