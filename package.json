{
  "name": "authorization",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "ISC",
  "main": "dist/index.js",
  "engineStrict": true,
  "engines": {
    "node": ">= 18.0.0"
  },
  "scripts": {
    "build": "swc src -d dist --source-maps --copy-files",
    "start": "node dist/index.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "build:tsc": "tsc && tsc-alias",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "seed": "knex seed:run --knexfile=knexfile.ts --verbose",
    "migrate": "knex migrate:latest --knexfile=knexfile.ts --verbose",
    "rollback": "knex migrate:rollback --knexfile=knexfile.ts --verbose",
    "make:seeder": "knex seed:make --knexfile=knexfile.ts -x ts --env development",
    "make:migration": "knex migrate:make --knexfile=knexfile.ts -x ts --env development",
    "clean": "rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.68",
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.4.1",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "cross-env": "^7.0.3",
    "nodemon": "^3.0.0",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.7",
    "tsconfig-paths": "^4.2.0"
  },
  "dependencies": {
    "@otedesco/cache": "^1.0.0",
    "@otedesco/commons": "^1.0.0",
    "@otedesco/notify": "^1.0.0",
    "@otedesco/server-utils": "^2.0.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "dotenv": "^16.3.1",
    "envalid": "^7.3.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "knex": "^2.5.0",
    "lodash": "^4.17.21",
    "objection": "^3.0.4",
    "objection-db-errors": "^1.1.2",
    "pg": "^8.11.1"
  }
}