{
  "name": "authorization",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "ISC",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run build && cross-env NODE_ENV=production node dist/index.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "build": "swc src -d dist --source-maps --copy-files",
    "test": "jest --forceExit --detectOpenHandles",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "npm run lint -- --fix",
    "lint:staged": "lint-staged",
    "seed": "knex seed:run --knexfile=knexfile.ts --verbose",
    "migrate": "knex migrate:latest --knexfile=knexfile.ts --verbose",
    "rollback": "knex migrate:rollback --knexfile=knexfile.ts --verbose",
    "make:seeder": "knex seed:make --knexfile=knexfile.ts -x ts --env development",
    "make:migration": "knex migrate:make --knexfile=knexfile.ts -x ts --env development",
    "docker:build": "docker build . -t authorization --target prod-server --build-arg NPM_TOKEN=$NPM_TOKEN"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.68",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.4.1",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.32.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "node-gyp": "^9.3.1",
    "nodemon": "^3.0.0",
    "prettier": "^2.8.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.7",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@otedesco/cache": "^2.0.0",
    "@otedesco/commons": "^2.0.0",
    "@otedesco/notify": "^2.0.0",
    "@otedesco/server-utils": "^3.0.1",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "dotenv": "^16.3.1",
    "envalid": "^7.3.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "knex": "^2.5.0",
    "lodash": "^4.17.21",
    "objection": "^3.0.4",
    "objection-db-errors": "^1.1.2",
    "pg": "^8.11.1"
  },
  "volta": {
    "node": "18.17.0"
  }
}
